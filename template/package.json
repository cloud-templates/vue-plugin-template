{
  "name": "{{ name }}",
  "description": "{{ description }}",
  "version": "{{ version }}",
  {{#unless_eq githubAccount ""}}
  "author": {
    "name": "{{ authorFullNameFrom author }}",
    "email": "{{ authorEmailFrom author }}"
  },
  {{/unless_eq}}
  "scripts": {
    "build": "node build/build.js",
    "lint": "eslint src"
  },
  {{#unless_eq githubAccount ""}}
  "repository": {
  "type": "git",
  "url": "git+https://github.com/{{ githubAccount }}/{{ name }}.git"
  },
  {{/unless_eq}}
  {{#unless_eq githubAccount ""}}
  "homepage": "https://github.com/Administrator/vue-search-stock#readme",
  {{/unless_eq}}
  "main": "dist/vue-search-stock.common.js",
  "keywords": [
    "plugin",
    "vue",
    "vuejs"
  ],
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-istanbul": "^3.1.2",
    "babel-polyfill": "6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-power-assert": "^1.0.0",
    "buble": "^0.14.0",
    "eslint": "^3.14.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-vue-libs": "^1.2.0",
    "html-webpack-plugin": "^2.19.0",
    "power-assert": "^1.4.2",
    "rollup": "^0.41.4",
    "rollup-plugin-alias": "^1.3.1",
    "rollup-plugin-buble": "^0.14.0",
    "rollup-plugin-less": "^0.1.3",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-vue": "^2.4.1",
    "uglify-js": "^2.7.5"
  },
  "files": [
    "dist/{{ name }}.js",
    "dist/{{ name }}.min.js",
    "dist/{{ name }}.esm.js",
    "dist/{{ name }}.common.js",
    "src"
  ],
  "license": "MIT",
  "engines": {
    "node": ">= 6.0"
  }
}
